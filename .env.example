# EconoNet Environment Configuration

# Operation Mode (offline/live/expert)
ECONET_MODE=live

# Default Country/Region
ECONET_COUNTRY=KE
ECONET_REGION=Africa/Nairobi

# Cache Settings
ECONET_CACHE=true
ECONET_CACHE_DIR=.econet_cache

# UI Settings
ECONET_SHOW_PROVENANCE=true
ECONET_SHOW_FALLBACKS=true

# Development Settings
ECONET_DEBUG=false
ECONET_LOG_LEVEL=INFO
